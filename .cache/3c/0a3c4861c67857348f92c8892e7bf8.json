s {\n  color: $color-font;\n  font-size: 20px;\n}\n%comments-settings {\n  border: 1px solid #fff;\n  padding: 15px 3px;\n  border-radius: 5px;\n  margin: 0;\n  width: 100%;\n  box-sizing: border-box;\n  height: 100%;  \n}\n\nbody {\n  margin: 0;\n  background: #8f57fd;\n}\n\n.container {\n  display: grid;\n  grid-template-rows: $height-header-footer $height-video auto $height-header-footer;\n  grid-template: minmax(320px, 1fr);\n  grid-row-gap: 5px;\n  background: #8f57fd;\n  color: $color-font;\n  font-family: $font-muli;\n  padding: 5px 15px;\n  align-content: space-evenly;\n  min-width: 320px;\n}\n\n//HEADER\n.header {\n  height: 70px;\n  display: grid;\n  grid-template-columns: auto 1fr auto;\n  grid-column-gap: 5px;\n  align-items: center;\n\n  figure {\n    margin: 0;\n    img {\n      vertical-align: top;\n      height: 70px;\n    }\n  }\n\n  .menu {\n    grid-column-start: 3;\n    display: flex;\n    align-items: center;\n    position: relative;\n    height: inherit;\n    cursor: pointer;\n\n    .menu__options {\n      margin: 0;\n      padding: 0;\n      text-align: right;\n      position: absolute;\n      top: 100%;\n      right: 0;\n      width: 110px;\n      display: none;\n\n      li {\n        list-style: none;\n        margin-bottom: 10px;\n        a {\n          @extend %a-settings;\n          letter-spacing: 0.5px;\n        }\n        a:hover {\n          @extend %a-transform;\n        }\n      }\n    }\n  }\n\n  .menu:hover .menu__options {\n    display: block;\n  }\n\n  .icon-user-circle:before {\n    content: \"\\e900\";\n    font-size: 2em;\n    margin: 0 7px;\n  }\n}\n//END HEADER\n\n//VIDEO\n.video {\n  display: grid;\n  grid-template-rows: repear(2, auto);\n  grid-row-gap: 5px;\n  justify-content: center;\n\n  .video__item {\n    width: 100%;\n    height: auto;\n  }\n\n  .video__controls {\n    display: grid;\n    grid-template-columns: repeat(3, auto);\n    grid-column-gap: 5px;\n    justify-content: start;\n  }\n\n  .button {\n    border: none;\n    border: 1px solid #fff;\n    border-radius: 5px;\n    width: 30px;\n    height: 30px;\n    padding: 0;\n    background: transparent;\n    background: #ab88ff;\n    outline: none;\n    cursor: pointer;\n  }\n}\n\n.icon {\n  @extend %icon-settings;\n}\n\n.icon-play2:before {\n  content: \"\\ea1c\";\n}\n.icon-pause:before {\n  content: \"\\ea1d\";\n}\n.icon-stop:before {\n  content: \"\\ea1e\";\n}\n.icon-unmute:before {\n  content: \"\\ea27\";\n}\n.icon-mute:before {\n  content: \"\\ea2a\";\n}\n//END VIDEO\n\n//START COMMENTS\n.comments {\n  display: grid;\n  grid-template-columns: 150px 1fr;\n  grid-auto-rows: 100px;\n  grid-gap: 10px;\n  justify-items: flex-start;  \n\n  h2 {\n    grid-column: 1 / span 2;\n  }\n\n  .comments__user {\n    font-weight: bold;\n    background: rgba(255, 255, 255, 0.4);\n    @extend %comments-settings;\n  }\n  .comments__user::after {\n    content: \":\";\n  }\n\n  .comments__detail {\n    @extend %comments-settings;\n    background: rgba(255, 255, 255, 0.25);\n  }\n}\n//END COMMNETS\n"},"lineCount":null}},"error":null,"hash":"06229be04754612200c4a11bfc783140","cacheData":{"env":{}}}