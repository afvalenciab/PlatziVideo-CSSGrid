n":24}}],"sources":{"../css/movies.scss":"//icons\n@import '../css/font-icon.css';\n\n//VARIABLES\n$color-font: #fff;\n$font-muli: \"Muli\", sans-serif;\n\n//Extend\n%a-settings {\n  display: inline-block;\n  text-decoration: none;\n  color: $color-font;\n  transition: 0.2s;\n  will-change: transform;\n}\n%a-transform {\n  text-decoration: underline;\n  transform: scale(1.04);\n}\n\nbody{\n  margin: 0;\n}\n\n.container {\n  background: #8f57fd;  \n  color: $color-font;\n  font-family: $font-muli;\n  padding: 5px 15px;\n  display: grid;\n  grid-template-rows: repeat(4, auto);\n  grid-row-gap: 5px;\n}\n\n//HEADER\n.header {\n  height: 70px;\n  display: grid;\n  grid-template-columns: auto 1fr auto;\n  grid-column-gap: 5px;\n  align-items: center;\n\n  figure {\n    margin: 0;\n    img {\n      vertical-align: top;\n      height: 70px;\n    }\n  }\n\n  .menu {\n    grid-column-start: 3;\n    display: flex;\n    align-items: center;\n    position: relative;\n    height: inherit;\n\n    .menu__options {\n      margin: 0;\n      padding: 0;\n      text-align: right;\n      position: absolute;\n      top: 100%;\n      right: 0;\n      width: 110px;\n      display: none;\n\n      li {\n        list-style: none;\n        margin-bottom: 10px;\n        a {\n          @extend %a-settings;\n          letter-spacing: 0.5px;\n        }\n        a:hover {\n          @extend %a-transform;\n        }\n      }\n    }\n  }\n\n  .menu:hover .menu__options {\n    display: block;\n  }\n\n  .icon-user-circle:before {\n    content: \"\\e900\";\n    font-size: 2em;\n    margin: 0 7px;\n  }\n}\n //END HEADER\n\n //SEARCH\n  .search{\n    height: 140px;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n    \n    label{\n      margin-bottom: 10px;\n      font-size: 1.5em;\n    }\n\n    .search__bar-container{\n      position: relative;\n      width: 65%;\n    }\n\n    .search__bar-input{      \n      height: 35px;\n      width: 100%;\n      outline: none;\n      border: 1px solid $color-font;\n      background: #AB88FF;\n      border-radius: 35px;\n      padding: 0 10px;\n      font-size: .9em;\n      color: $color-font;\n      font-family: $font-muli; \n      position: relative;\n      box-sizing: border-box;\n    }\n\n    .search__bar-input::placeholder{\n      color: $color-font;\n    }\n  }\n\n  .icon-search::after {\n    content: \"\\e901\";\n    position: absolute;\n    top: 0;\n    right: 10px;\n    font-size: 1.2em;\n    font-weight: bold;\n    line-height: 35px;\n  }\n //END SEARCH\n\n //MAIN\n.movies{\n  display: grid;\n  grid-auto-rows: 50px 350px;\n  grid-gap: 5px;\n  align-items: center;\n\n  h3{\n    margin: 0;\n  }\n\n  .carrousel__list{\n    display: grid;\n    grid-auto-columns: 200px;\n    grid-column-gap: 10px;\n    grid-auto-flow: column;\n    overflow-x: auto;\n    padding: 50px 0;\n\n    .carrousel__item{\n      border-radius: 20px;\n      overflow: hidden;\n      position: relative;\n      transition: .3s all;\n      transform-origin: center left;\n      cursor: pointer;\n\n      figure{\n        margin: 0; \n        img{\n          vertical-align: top;\n          display: inline-block;\n          width: 200px;\n          height: 250px;\n        }\n      }\n    }\n\n    .carrousel__item:hover{\n      transform: scale(1.4);\n      opacity: 1;\n    }\n    .carrousel__item:hover ~ .carrousel__item{\n      transform: translateX(80px);\n    }\n    .carrousel__item:hover  .carrousel__item-cover{\n      display: block;\n    }\n\n    .carrousel__item-cover{\n      position: absolute;\n      width: 200px;      \n      bottom: 0;  \n      padding: 10px;\n      box-sizing: border-box;\n      background: linear-gradient( transparent, rgba(0,0,0,.9) );\n      display: none;\n\n      p{\n        margin: 0;\n      }\n\n      .tittle__movie{\n        font-size: 16px;\n        font-weight: 600px;\n      }\n\n      .detail__movie{\n        font-size: 12px;\n      }\n    }\n\n  }\n}\n\n.carrousel__list:hover .carrousel__item{\n  opacity: .5;\n}\n\n.icon{\n  display: inline-block;    \n  color: #21C08B;\n  font-size: 18px;\n  font-weight: bold;\n  border-radius: 50%;  \n  background: #FFF;\n}\n\n.icon-plus:before {\n  content: \"\\e902\";  \n}\n\n.icon-play:before {\n  content: \"\\e903\";\n}\n //END MAIN\n\n //FOOTER\n.footer{\n  height: 70px;\n  display: grid;\n  grid-template-columns: repeat(3, auto);\n  grid-column-gap: 15px;\n  align-content: center;\n  justify-content: start;\n  a{\n    @extend %a-settings;\n  }\n  a:hover{\n    @extend %a-transform;\n  }\n\n}\n //END FOOTER"},"lineCount":null}},"error":null,"hash":"cb2847d1c096fb7c06b874497f35d2be","cacheData":{"env":{}}}