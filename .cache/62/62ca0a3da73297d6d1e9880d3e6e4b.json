down: #8f57fd;\n$height-header: 70px;\n$height-login: minmax(357px, calc(100vh - 170px));\n$font-Muli: \"Muli\", sans-serif;\n$color-font: #fff;\n\n//Extends/Inheritance\n%flex-settings {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n%a-settings {\n  text-decoration: none;\n  color: $color-font;\n  font-size: 0.9em;\n  transition: 0.1s;\n  will-change: transform;\n}\n%a-transform {\n  text-decoration: underline;\n  transform: scale(1.02);\n  transform-origin: 0 100%;\n}\n\nbody {\n  margin: 0;\n}\n\n.container {\n  display: grid;\n  grid-gap: 5px;\n  grid-template: $height-header $height-login 70px / 1fr 320px 1fr;\n  grid-template-areas:\n    \"header header header\"\n    \". login .\"\n    \"footer footer footer\";\n\n  background: linear-gradient($background-color-up, $background-color-down);\n  padding: 10px;\n  box-sizing: border-box;\n  font-family: $font-Muli;\n  color: $color-font;\n}\n\n// START HEADER\n.header {\n  grid-area: header;\n  height: $height-header;\n  figure {\n    height: inherit;\n    margin: 0;\n    img {\n      display: block;\n      height: inherit;\n    }\n  }\n}\n// END HEADER\n\n// START LOGIN\n.login {\n  grid-area: login;\n  display: grid;\n  grid-template-rows: repeat(4, auto);\n  grid-row-gap: 5px;\n  align-items: center;\n  align-content: space-evenly;\n  font-size: 16px;\n  border: 1px solid $color-font;\n  border-radius: 25px;\n  padding: 0 30px;\n  background: rgba(255, 255, 255, 0.1);\n\n  h2 {\n    margin: 0;\n    margin: 15px 0;\n    font-size: 1.3em;\n  }\n\n  form {\n    > input {\n      display: block;\n      width: 100%;\n      box-sizing: border-box;\n      margin-bottom: 15px;\n      outline: none;\n      border: 0;\n      border-bottom: 1px solid $color-font;\n      background: transparent;\n      padding: 10px;\n      font-size: 0.9em;\n      color: $color-font;\n      font-family: $font-Muli;\n    }\n    input::placeholder {\n      color: $color-font;\n      font-weight: 100;\n    }\n\n    button {\n      width: 100%;\n      border: 0;\n      height: 40px;\n      border-radius: 50px;\n      outline: none;\n      font-family: $font-Muli;\n      font-size: 0.9em;\n      letter-spacing: 1px;\n      background: rgba(255, 255, 255, 0.4);\n      color: $color-font;\n      margin-bottom: 15px;\n      cursor: pointer;\n      transition: 0.3s;\n      will-change: transform;      \n    }\n    button:active {\n      transform: scale(0.9);\n    }\n\n    .login__options {\n      font-size: 0.9em;\n      @extend %flex-settings;\n      justify-content: space-between;\n      margin-bottom: 15px;\n      a {\n        display: inline-block;\n        font-size: 1em;\n        @extend %a-settings;\n      }\n      a:hover {\n        @extend %a-transform;\n      }\n\n      input[type=\"checkbox\"] {\n        margin: 0;\n        width: 14.3px;\n        height: 14.3px;        \n      }\n    }\n  }\n\n  .login__social {\n    a {\n      display: block;\n      margin-bottom: 10px;\n      @extend %a-settings;\n    }\n    a:hover {\n      @extend %a-transform;\n    }\n  }\n\n  .icon-google:before {\n    content: \"\\ea88\";\n  }\n\n  .icon-twitter:before {\n    content: \"\\ea96\";\n  }\n\n  .login__singup {\n    font-size: 0.9em;\n    p {\n      margin: 0;\n      margin-bottom: 15px;\n    }\n    a {\n      display: inline-block;\n      @extend %a-settings;\n      font-size: 1em;\n      font-weight: 600;\n    }\n    a:hover {\n      @extend %a-transform;\n    }\n  }\n}\n// END LOGIN\n\n// START FOOTER\n.footer {\n  grid-area: footer;\n  @extend %flex-settings;\n  justify-content: flex-start;\n\n  a {\n    @extend %a-settings;\n    margin-right: 10px;\n  }\n  a:hover {\n    @extend %a-transform;\n  }\n}\n// END FOOTER\n\n//MEDIA QUERIES\n@media screen and (max-width: 480px) {\n  $height-header: 50px;\n  $height-login: minmax(357px, calc(100vh - 150px));\n\n  .container {\n    grid-template: $height-header $height-login 70px / minmax(320px, 1fr);\n    grid-template-areas:\n      \"header\"\n      \"login\"\n      \"footer\";\n  }\n\n  .header {\n    height: $height-header;\n    figure {\n      height: inherit;\n      img {\n        height: inherit;\n      }\n    }\n  }\n\n  .login {\n    font-size: 14px;\n    border: initial;\n    border-radius: initial;\n    padding: 0;\n    background: none;\n    \n    .login__social {\n      a {\n        text-align: center;\n      }\n    }\n\n    .login__singup {\n      text-align: center;\n    }\n  }\n\n  .footer {\n    flex-direction: column;    \n    a {\n      @extend %a-settings;\n      margin-right: 0;\n      margin-bottom: 10px;\n      display: block;\n      text-align: center;      \n    }\n    a:hover {\n      @extend %a-transform;\n    }\n    a:last-child{\n      margin-bottom: 0;\n    }\n  }\n}\n//END MEDIA QUERIES\n"},"lineCount":null}},"error":null,"hash":"acca01649a191509f838a3174e0385de","cacheData":{"env":{}}}